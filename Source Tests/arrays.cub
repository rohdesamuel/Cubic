# Multi-dimensional arrays
val d: int[int[2]] = [[1,2], [3, 4]]
assert d[0][0] == 1
assert d[0][1] == 2
assert d[1][0] == 3
assert d[1][1] == 4

assert d[0] == a
assert a == d[0]

# Declarations
val e: int[2]
e = [1, 2]

assert e[0] == 1
assert e[1] == 2
assert e == a
assert a == e

