do
  val accum: int = 0
  # 1 + 2 + 3 + 4 + 5
  for i in 1..5 do
    accum = accum + i
  end
  assert accum == 15
end

do
  val accum: int = 0
  # 1 + 3 + 5
  for i in 1..5 step 2 do
    accum = accum + i
  end
  assert accum == 9
end

do
  val accum: int = 0
  # 1 + 3 + 5
  for i in 1..5 step 2 do
    accum = accum + i
  end
  assert accum == 9
end

for i = 1 and i < 5 step i = i + 1