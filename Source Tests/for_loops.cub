do
  val accum: int = 0
  # 1 + 2 + 3 + 4
  for i in 1..5 do
    accum = accum + i
  end
  assert accum == 10
end

do
  val accum: int = 0
  # 1 + 3
  for i in 1..5 step 2 do
    accum = accum + i
  end
  assert accum == 4
end

do
  val accum: int = 0
  # 1 + 3
  for i in 1..5 step 2 do
    accum = accum + i
  end
  assert accum == 4
end